{
  "compilerOptions": {
    "outDir": "./dist/",
    "noImplicitAny": true,
    "module": "esnext",
    "target": "es5",
    "jsx": "react-jsx",
    "allowJs": true,
    "moduleResolution": "node",
    "baseUrl": ".",
    "paths": {
      "*":["./src/*"]
    },
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    ///
    "strictNullChecks": false
  },
  "ts-node": {
      "compilerOptions": {
        "module": "CommonJS"
      }
    },
    "include": ["**/*.ts", "**/*.tsx", ".eslintrc.js", "./config/jest/setupTests.ts"],
  }